 

 albums = {
    _id: ObjectId,
    name: String,
    artist: String,
    url: String,
    playcount: String,
    listeners: String,
    tags: Array,
    images: {
      small: String,
      medium: String,
      large: String,
      extralarge: String,
      mega: String
    },
    tracks: Array [
        {
          name: String,
          duration: Number,
          rank: Number,
          url: String,
          artist: String
        }
      ],
    wiki_summary: String
  }

artists = {
    _id: ObjectId,
    name: String,
    tags: Array,
    similar_artists: Array,
    wiki: {
      published: String,
      summary: String,
      content: String
    },
    musicbrainz_id: String/UUID
  } 

tracks_metadata = {
    _id: ObjectId,
    artist: String,
    track: String,
    album: String,
    metadata: {
      prompted_artist: String,
      prompted_track: String,
      musicbrainz: {
        recording_id: String,
        title: String,
        artist: String,
        artist_id: String,
        album: String,
        album_id: String,
        release_date: String,
        country: String,
        length: Number,
      },
      lastfm: {
        tags: Array,
        similar_tracks: Array
      },
      genius: {
        title: String,
        artist: String,
        album: String,
        release_date: String,
        song_art_image_url: String,
        verified: Boolean
      }
    },
  }
